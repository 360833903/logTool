    <div class='left'>
      <h1>查询</h1>
      <input type='hidden' id='data'></input>
      <input type='hidden' id='page'></input>
      <input type='hidden' id='total'></input>
      <input type='hidden' id='catetemp'></input>
      <form method='post'>
        <span>版本</span><br/>
        <select name='area' id='area' onchange='checkWorld(["itemgroup_info","lucky_ring_list"],0);'>
          <option value='0'>请选择</option>
          <option value='1'>简体</option>
          <option value='2'>繁体</option>
