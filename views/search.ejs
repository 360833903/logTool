        <span>wuid</span><br />
        <input type='text' name='wuid' id='wuid'></input><br />
        <span>日志类型</span><br />
        <select name='category' id='category'>
          <option value='0'>请选择</option>
          <option value='1'>钻石</option>
          <option value='2'>
            卡牌</option>
	   <option value='3'>球票</option>
	   <option value='4'>碎片</option>
	   <option value='5'>转盘</option>
	   <option value='6'>特训卡</option>
        </select><br />
        <span>开始时间</span><br />
        <input type='date' name='start' id='start' /><br />
        <span>结束时间</span><br />
        <input type='date' name='end' id='end' /><br />
        <input type='submit' onclick='check();return false;' /><br />
      </form>
      <span id='addr'></span>
      <table>
        <tr>
          <td><button id="prevpagebt" disabled="true" onclick="prevpage()">上页</button></td>
          <td><button id="nextpagebt" disabled="true" onclick="nextpage()">下页</button></td>
        </tr>
